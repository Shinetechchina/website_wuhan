<li class='clearfix record <%= cycle("on", "on-hover") %>' id="<%= dom_id(staff) -%>">
  <span class='title'>
    <%= staff.name %>

  </span>
  <span class='actions'>
    
    <%= link_to refinery_icon_tag("application_go.png"), refinery.staffs_staff_path(staff),
        :title => t('.view_live_html'),
        :target => "_blank" %>
    
    <%= link_to refinery_icon_tag("application_edit.png"), refinery.edit_staffs_admin_staff_path(staff),
         :title => t('.edit') %>
    <%= link_to refinery_icon_tag("delete.png"), refinery.staffs_admin_staff_path(staff),
        :class => "cancel confirm-delete",
        :title => t('.delete'),
        :confirm => t('message', :scope => 'refinery.admin.delete', :title => staff.name),
        :method => :delete %>
  </span>
</li>
